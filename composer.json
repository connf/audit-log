{
    "name": "Connf/Auditlog",
    "version": "1.0",
    "type": "library",
    "description": "An Audit logger developed by Connah Fearnley which is a fork of the Spatie Activitylog package.",
    "keywords":
    [
        "log",
        "user",
        "audit",
        "laravel"
    ],
    "homepage": "https://github.com/connf/auditlog",
    "authors": [
        {
            "name": "Connah Fearnley",
            "role": "Code Porting Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/config": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0",
        "illuminate/database": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0",
        "illuminate/support": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0",
        "spatie/string": "^2.1"

    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "orchestra/testbench": "~3.3.0|~3.4.0",
        "orchestra/database": "~3.3.0|~3.4.0"
    },
    "autoload": {
        "psr-4": {
            "Connf\\Auditlog\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Connf\\Auditlog\\Test\\": "tests"
        }
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "Conf\\Auditlog\\AuditlogServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
